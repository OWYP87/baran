version: '3'
services:
  {{ web_app_name }}:
    image: "szalasartur/python_app:{{ image_version }}"
    ports:
      - "8000:8000"
    environment:
      POSTGRES_DB: "{{ database_name }}"
      POSTGRES_USER: "{{ postgres_user }}"
      POSTGRES_PW: "{{ postgres_pass }}"
      POSTGRES_URL: "{{ database_ip }}:5432"

